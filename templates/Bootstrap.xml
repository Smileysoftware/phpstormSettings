<templateSet group="Bootstrap">
  <template name="bcontainer" value="&lt;div class=&quot;container&quot;&gt;&#10;  &#10;    $END$&#10;  &#10;&lt;/div&gt;" description="Bootstrap Container" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="bcss" value="&lt;link rel=&quot;stylesheet&quot; href=&quot;https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css&quot;&gt;" description="Bootstrap CSS" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="bjs" value="&lt;script src=&quot;https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js&quot;&gt;&lt;/script&gt;" description="Bootstrap JS" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="brow" value="&lt;div class=&quot;row&quot;&gt;&#10;&#10;    $END$&#10;&#10;&lt;/div&gt;" description="Bootstrap row" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="bcol" value="&lt;div class=&quot;col-sm-$small$ col-md-$medium$ col-lg-$large$&quot;&gt;&#10;&#9;&#10;&#9;$END$&#10;&#10;&lt;/div&gt;" description="Bootstrap Column" toReformat="false" toShortenFQNames="true">
    <variable name="small" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="medium" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="large" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="btable" value="&lt;table class=&quot;table table-hover table-bordered table-striped&quot;&gt;&#10;    &lt;thead&gt;&#10;        &lt;tr&gt;&#10;            &lt;td&gt;$column1title$&lt;/td&gt;&#10;            &lt;td&gt;$column2title$&lt;/td&gt;&#10;        &lt;/tr&gt;&#10;    &lt;/thead&gt;&#10;    &lt;tfoot&gt;&#10;        &lt;tr&gt;&#10;            &lt;td&gt;$column1title$&lt;/td&gt;&#10;            &lt;td&gt;$column2title$&lt;/td&gt;&#10;        &lt;/tr&gt;&#10;    &lt;/tfoot&gt;&#10;    &lt;tbody&gt;&#10;        &lt;tr&gt;&#10;            &lt;td&gt;$column1data$&lt;/td&gt;&#10;            &lt;td&gt;$column2data$&lt;/td&gt;&#10;        &lt;/tr&gt;&#10;    &lt;/tbody&gt;&#10;&lt;/table&gt;" description="Bootstrap Table" toReformat="false" toShortenFQNames="true">
    <variable name="column1title" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="column2title" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="column1data" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="column2data" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="bformfield" value="&lt;div class=&quot;form-group&quot;&gt;&#10;    &lt;label for=&quot;$name$&quot;&gt;$Nice_name$&lt;/label&gt;&#10;    &lt;input type=&quot;$type$&quot; class=&quot;form-control&quot; id=&quot;$name$&quot; placeholder=&quot;Please enter your $placeholder$&quot;&gt;&#10;&lt;/div&gt;" description="Bootstrap Form Field" toReformat="false" toShortenFQNames="true">
    <variable name="name" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="type" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="Nice_name" expression="capitalize(name)" defaultValue="" alwaysStopAt="true" />
    <variable name="placeholder" expression="capitalize(name)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="bfield" value="&lt;div class=&quot;form-group&quot;&gt;&#10;    &lt;label for=&quot;$name$&quot;&gt;$title$&lt;/label&gt;&#10;    &lt;input type=&quot;$type$&quot; name=&quot;$name$&quot; value=&quot;{{ old('$name$') }}&quot; class=&quot;form-control&quot;&gt;&#10;&lt;/div&gt;" description="Bootstrap Form Field" toReformat="false" toShortenFQNames="true">
    <variable name="name" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="type" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="title" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
</templateSet>